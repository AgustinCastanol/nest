{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest",
  "projects": {
    "nest": {
      "type": "application",
      "root": "apps/nest",
      "entryFile": "main",
      "sourceRoot": "apps/nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest/tsconfig.app.json"
      }
    },
    "login": {
      "type": "application",
      "root": "apps/login",
      "entryFile": "main",
      "sourceRoot": "apps/login/src",
      "compilerOptions": {
        "tsConfigPath": "apps/login/tsconfig.app.json"
      }
    },
    "aph": {
      "type": "application",
      "root": "apps/aph",
      "entryFile": "main",
      "sourceRoot": "apps/aph/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aph/tsconfig.app.json"
      }
    }
  }
}